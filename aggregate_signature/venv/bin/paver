#!/Users/alberto/Desktop/aggregate_signature/bplib/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Paver==1.2.4','console_scripts','paver'
__requires__ = 'Paver==1.2.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Paver==1.2.4', 'console_scripts', 'paver')()
    )
